<%- include('./partials/navbar'); %>
<br />

<h1 class="bg-dark text-white">Listado de productos</h1>
<% prod.forEach((item)=>{ %>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">NOMBRE</th>
      <th scope="col">PRECIO</th>
      <th scope="col">FOTO</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>id: <%=item.id%><br /></td>
      <td>titulo: <%=item.title%></td>
      <td>price: <%=item.price%><br /></td>
      <td>
        <img
          src="<%=item.thumbnail %>"
          class="img-thumbnail"
          style="width: 15%"
        />
      </td>
    </tr>
  </tbody>
</table>
<% }) %>
<a href="/form">
  <button class="btn btn-secondary">VOLVER AL FORMULARIO</button>
</a>
<br />
<%- include('./partials/footer'); %>
<br />

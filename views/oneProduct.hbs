{{> navbar}}
<br />
<h1 class='bg-dark text-white'>Listado de productos agregados</h1>
<table class='table table-dark'>
  <thead>
    <tr>
      <th scope='col'>ID</th>
      <th scope='col'>NOMBRE</th>
      <th scope='col'>PRECIO</th>
      <th scope='col'>FOTO</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{body.id}} </td>
      <td> {{body.name}}</td>
      <td>{{body.price}} </td>
      <td><img src='{{body.picture}}' class='img-thumbnail' style='width: 15%;' /></td>
    </tr>
  </tbody>
</table>
<a href='/api/form'>
  <button class='btn btn-secondary'>VOLVER AL FORMULARIO</button>
</a>
<br />
{{> footer}}
<br />